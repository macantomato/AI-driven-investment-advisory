{
  "scriptFile": "function_app.py",
  "entryPoint": "main",
  "bindings": [
    {
      "type": "httpTrigger",
      "authLevel": "anonymous",
      "direction": "in",
      "name": "req",
      "methods": [ "get", "post" ],
      "route": "{*segments}" 
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}